package tesla;

import processing.core.PApplet;
import processing.core.PImage;

public class usuarioRegistrado extends pantallas {
	
	private PApplet app1;
	private PImage pantallaInicio;

	public usuarioRegistrado(int posX, int posY, int tamX, int tamY) {
		super(posX, posY, tamX, tamY);
	}

	

	

	public void crearPantalla() {
		
		pantallaInicio = app1.loadImage("tesla/imagenes/fondoInicio.jpg");
		app1.image (pantallaInicio, posX, posY, tamX, tamY);
		
	}
	
	public void cambioPantalla() {
		
	}

}
